{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@databricks/web-shared-bundle/*": ["./src/__generated__/web-shared-bundle/*"]
    },
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "lib": ["es6", "dom", "es2016", "es2017"],
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "suppressImplicitAnyIndexErrors": false,
    "composite": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "incremental": true,
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "preserveWatchOutput": true,
    "resolveJsonModule": true,
    "outDir": "lib",
    "rootDir": "src",
    "strict": true,
    "strictNullChecks": true,
    "target": "ES2018",
    "allowJs": true,
    "types": ["jest"],
    "jsxImportSource": "@emotion/react"
  },
  "include": ["./src/**/*","./src/i18n/default/cn.json"],
}
